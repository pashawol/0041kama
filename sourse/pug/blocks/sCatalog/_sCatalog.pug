				+card-item2('1','col-md-4 col-6')
				+card-item2('2','col-md-4 col-6')
				+card-item2('3','col-md-4 col-6','Двигатель 740.11 (740.11- 1000400)')
				+card-item2('3','col-md-4 col-6','Двигатель 740.11 (740.11- 1000400)')
				+card-item2('1','col-md-4 col-6')
				+card-item2('2','col-md-4 col-6')
				+card-item2('1','col-md-4 col-6')
				+card-item2('2','col-md-4 col-6')
				+card-item2('3','col-md-4 col-6','Двигатель 740.11 (740.11- 1000400)')
