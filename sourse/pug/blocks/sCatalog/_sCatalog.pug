mixin sCatalog(data)
	// start sCatalog
	+b.SECTION.sCatalog.section#sCatalog&attributes(attributes)
		.container
			+b.section-title.text-center
				h2 Популярные товары	
			.row
				+card-item('1')
				+card-item('2')
				+card-item('3','Двигатель 740.11 (740.11- 1000400)')
				+card-item('1')
				+card-item('3','Двигатель 740.11 (740.11- 1000400)')
				+card-item('1')
				+card-item('1')
				+card-item('3','Двигатель 740.11 (740.11- 1000400)')
	// end sCatalog

